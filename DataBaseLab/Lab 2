1. select name,dept_name from student;
2. select name from instructor where dept_name='Comp. Sci.';
3. delect title from course where dept_name='Comp. Sci.' and credits=3;
4. select course.course_id,title from student1,course,takes where student1.id=takes.id and course.course_id=takes.course_id and student1.id=12345;
5. select name from instructor where salary between 40000 and 90000;
6. update advisor set i_id=10101 where s_id between 10000and 20000;
7. update instructor set dept_name='Comp. Sci.' where dept_name='Biology';
8.update instructor set salary=salary*1.05;
9.delete from department where dept_name='Music';
10. delete from prereq where course_id='CS-101';
11. delete from course where course_id not in (select distinct course_id from takes);
12. alter table student1 add (DOB date);
13. alter table department modify (dept_name varchar(20));
14. alter table student1 drop column student1;
15. select ID from instructor where ID not in (select ID from teaches);
18. select distinct T.name,T.salary as inst_sal from instructor T,instructor S where T.salary>S.salary and S.dept_name='Comp. Sci.';
19. select name from instructor where dept_name like '%Com%';
